{
  "name": "deku-slideout",
  "version": "1.0.1",
  "description": "Slideout component for Deku",
  "license": "MIT",
  "repository": "kevva/deku-slideout",
  "author": {
    "name": "Kevin Martensson",
    "email": "kevinmartensson@gmail.com",
    "url": "github.com/kevva"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "build": "browserify example/index.js -o example/build.js -dv",
    "prepublish": "babel --presets es2015,react --out-file dist.js index.js",
    "test": "xo"
  },
  "main": "dist.js",
  "files": [
    "dist.js"
  ],
  "keywords": [
    "deku",
    "deku-component",
    "dekujs",
    "component"
  ],
  "dependencies": {
    "click-outside": "^1.0.4",
    "magic-virtual-element": "^1.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "deku": "^1.0.0",
    "eslint-config-xo-react": "^0.3.0",
    "eslint-plugin-react": "^3.15.0",
    "xo": "*"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "xo": {
    "envs": [
      "browser",
      "node"
    ],
    "esnext": true,
    "extends": [
      "xo-react"
    ],
    "ignores": [
      "dist.js",
      "example/**"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "no-nested-ternary": 0,
      "react/no-unknown-property": 0,
      "react/prop-types": 0,
      "quote-props": 0
    }
  }
}
